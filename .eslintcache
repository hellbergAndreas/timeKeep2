e => {\n            return {\n              ...prevState,\n              [res.id]: category,\n            }\n          })\n          setUserCategories(prevState => {\n            return [...prevState, category]\n          })\n        })\n    } else {\n      inputs = {\n        ...inputs,\n        parent: category.id,\n      }\n      // first send to db\n      userKit\n        .addActivity(inputs)\n        .then(res => {\n          if (res.status <= 200) {\n            return res.json()\n          }\n        })\n        //then update local state to match db\n        .then(res => {\n          let activity = {\n            description: inputs.description,\n            name: inputs.activity,\n            parent: category.id,\n            userId: currentUser.uid,\n            id: res.id,\n          }\n          setActivitiesObject(prevState => {\n            return {\n              ...prevState,\n              [res.id]: activity,\n            }\n          })\n          setUserActivities(prevState => {\n            return [...prevState, activity]\n          })\n        })\n    }\n    setHidden(!hidden)\n  }\n  const handleChange = (name, value) => {\n    setInputValues(prevState => {\n      return {\n        ...prevState,\n        [name]: value,\n      }\n    })\n  }\n  return (\n    <div className={styles.background}>\n      <div className={styles.shape}></div>\n      <div className={styles.formWrapper}>\n        <button className={styles.btn} onClick={() => setHidden(!hidden)}>\n          X\n        </button>\n        <FormInput\n          required\n          label={type === \"categories\" ? \"Category name\" : \"Activity name\"}\n          handleChange={handleChange}></FormInput>\n        <FormInput\n          required\n          handleChange={handleChange}\n          label=\"Description\"></FormInput>\n        <div className={styles.submit}>\n          <Button onClick={handleClick}>Add</Button>\n        </div>\n      </div>\n    </div>\n  )\n}\n","/Users/andreashellbe