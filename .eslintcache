ctivity(inputs).then((res) => {\n        if (res.status <= 200) {\n          setUserActivities((prevState) => {\n            return [\n              ...prevState,\n              {\n                description: inputs.description,\n                name: inputs.activity,\n                parent: category,\n                userId: currentUser.uid,\n              },\n            ]\n          })\n        }\n      })\n    }\n  }\n  const handleChange = (name, value) => {\n    setInputValues((prevState) => {\n      return {\n        ...prevState,\n        [name]: value,\n      }\n    })\n  }\n  return (\n    <div className={styles.background}>\n      <div className={styles.formWrapper}>\n        <button className={styles.button} onClick={() => setHidden(!hidden)}>\n          X\n        </button>\n        <FormInput\n          required\n          label={type === \"categories\" ? \"Category name\" : \"Activity name\"}\n          handleChange={handleChange}\n        ></FormInput>\n        <FormInput\n          required\n          handleChange={handleChange}\n          label=\"Description\"\n        ></FormInput>\n        <Button onClick={handleClick}>Add</Button>\n      </div>\n    </div>\n  )\n}\n","/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/context/CategoryContext.js",[],"/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/components/Buttons/StartButton.jsx",["192","193","194","195","196","197"],"import React, { useEffect, useState } from \"react\"\nimport ConfirmSession from \"../../containers/ConfirmSession/ConfirmSession\"\nimport { useAuth } from \"../../context/AuthContext\"\nimport { useCategory } from \"../../context/CategoryContext\"\nimport { useSession } from \"../../context/SessionContext\"\nimport UserKit from \"../../data/UserKit\"\nimport Button, { ButtonShape } from \"./Button\"\nimport styles from \"./StartButton.module.scss\"\nconst StartButton = () => {\n  const {\n    category,\n    activity,\n    setSession,\n    