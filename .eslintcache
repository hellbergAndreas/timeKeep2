n    //   scrollAmount = 0\n    // }\n  }\n\n  const handleClick = (id, name) => {\n    if (type === \"categories\") {\n      setCategory({ id, name })\n      category.id === id && setCategory({ id: null })\n      setActivity({ id: null })\n    }\n    if (type === \"activities\") {\n      setActivity({\n        id,\n        name,\n      })\n      activity.id && activity.id === id && setActivity({ id: null })\n    }\n  }\n\n  const renderCategories = () => {\n    if (list.length > 0) {\n      return list.map((item, i) => {\n        return (\n          <ListObject\n            delay={i}\n            key={item.id}\n            id={item.id}\n            category={category}\n            activity={activity}\n            timeGoes={timeGoes}\n            canBeDeactivated={true}\n            name={item.name}\n            onClick={handleClick}>\n            {item.name}\n          </ListObject>\n        )\n      })\n    }\n  }\n  return (\n    <section className={styles.carousel}>\n      <div ref={sliders} className={styles.carouselBox}>\n        {list && renderCategories()}\n        <div\n          onClick={slideLeft}\n          className={cx(styles.switchLeft, styles.sliderButton)}>\n          {\"<\"}\n        </div>\n        <div\n          onClick={slideRight}\n          className={cx(styles.switchRight, styles.sliderButton)}>\n          {\">\"}\n        </div>\n      </div>\n    </section>\n  )\n}\n\nexport default CategoryContainer\n","/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/components/ListObject/ListObject.jsx",[],"/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/containers/ListHeaderContainer/ListHeaderContainer.jsx",["262","263","264","265"],"import React, { useState } from \"react\"\nimport Button from \"../../components/Buttons/Button\"\nimport Header from \"../../components/Header/Header\"\nimport { useSession } from \"../../context/SessionContext\"\n\nimport FormCard from \"../FormCard/FormCard\"\nimport styles from \