    // sliders.scrollTo({\n    //   top: 0,\n    //   left: (scrollAmount -= 100),\n    //   behavior: \"smooth\",\n    // })\n\n    // if (scrollAmount > 0) {\n    //   scrollAmount = 0\n    // }\n  }\n\n  const handleClick = (id, name) => {\n    if (type === \"categories\") {\n      setCategory({ id, name })\n      category.id === id && setCategory({ id: null })\n      setActivity({ id: null })\n    }\n    if (type === \"activities\") {\n      setActivity({\n        id,\n        name,\n      })\n      activity.id && activity.id === id && setActivity({ id: null })\n    }\n  }\n\n  const renderCategories = () => {\n    if (list.length > 0) {\n      return list.map((item) => {\n        return (\n          <ListObject\n            key={item.id}\n            id={item.id}\n            category={category}\n            activity={activity}\n            timeGoes={timeGoes}\n            canBeDeactivated={true}\n            name={item.name}\n            onClick={handleClick}\n          >\n            {item.name}\n          </ListObject>\n        )\n      })\n    }\n  }\n  return (\n    <section className={styles.carousel}>\n      <div ref={sliders} className={styles.carouselBox}>\n        {list && renderCategories()}\n        <div\n          onClick={slideLeft}\n          className={cx(styles.switchLeft, styles.sliderButton)}\n        >\n          {\"<\"}\n        </div>\n        <div\n          onClick={slideRight}\n          className={cx(styles.switchRight, styles.sliderButton)}\n        >\n          {\">\"}\n        </div>\n      </div>\n    </section>\n  )\n}\n\nexport default CategoryContainer\n","/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/components/ListObject/ListObject.jsx",[],"/Users/andreashellberg/Desktop/programmeringsprojekt/timekeepv2/src/containers/ListHeaderContainer/ListHeaderContainer.jsx",["245","246","247","248"],"import React, { useState } from \"react\"\nimport Button from \"../../components/Buttons/Button\"\nimport Header from 